{
    "name": "zamzar/zamzar-php",
    "type": "library",
    "minimum-stability": "stable",
    "description": "Zamzar PHP Library",
    "keywords": [
        "zamzar",
        "file conversion",
        "api"
    ],
    "homepage": "https://www.zamzar.com/",
    "license": "Apache-2.0",
    "authors": [
        {
          "name": "Zamzar",
          "homepage": "https://github.com/zamzar/zamzar-php"
        }
    ],
    "require": {
        "php": ">=7.2.34",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
          "Zamzar\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Tests\\": "tests/"
        }
    }
}
